<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nyc.c4q.leighdouglas.foodtogo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".hakeem.SignUpActivityFirebase"/>
        <activity android:name=".leigh.RestaurantListActivity"/>
        <activity android:name=".leigh.RestaurantProfileActivity"/>
        <activity android:name=".hyunjoo.RunnerActivity"/>
        <activity android:name=".hyunjoo.dropoff.DropOffListActivity"/>
        <activity android:name=".hakeem.LoginActivityFirebase"/>

    </application>

</manifest>